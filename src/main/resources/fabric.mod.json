{
  "schemaVersion": 1,
  "id": "foxclient",
  "version": "${version}-rev.#branch#.#githash#",
  "name": "FoxClient",
  "description": "A Fabric (and Quilt) Mod that changes the look of the Minecraft UI and adds some features.",
  "authors": [
    "Rooot",
    "Flustix"
  ],
  "contributors": [
    "PassiHD2004"
  ],
  "contact": {
    "homepage": "https://client.foxes4life.net/",
    "sources": "https://github.com/FoxStudios/FoxClient",
    "issues": "https://github.com/FoxStudios/FoxClient/issues"
  },
  "license": "MIT",
  "icon": "assets/foxclient/icon.png",
  "environment": "client",
  "entrypoints": {
    "main": [
      "net.foxes4life.foxclient.Main"
    ],
    "client": [
      "net.foxes4life.foxclient.MainClient"
    ]
  },
  "mixins": [
    "foxclient.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.13.2",
